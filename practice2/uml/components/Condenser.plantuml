@startuml

class Condenser:
{
    +energy: string
    +type: string
    +name: 设备名称
    +inNode: 冷凝器入口节点名称
    +outNode: 冷凝器出口节点名称
    +iNode: 入口节点参数
    +oNode: 出口节点参数
    +heatExtracted: 放热量（kJ/kg）
    +QExtracted: 放热功率（MW）
    +__init__(): 初始化
    +state(): 状态参数计算
    +balance(): 平衡计算
    +sm_energy(): 功率计算
    +__str__(): 结果输出
}

@enduml